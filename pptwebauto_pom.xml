<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

	 <parent>
		<groupId>com.gwf</groupId>
		<artifactId>enterprise-parent-pom-java</artifactId>
		<version>1.2</version>
	</parent> 
	<groupId>com.fascore.qa.ParticipantWebAuto</groupId>
	<artifactId>pptwebAuto</artifactId>
	<version>0.0.1-PPTWebSNAPSHOT</version>
	<packaging>jar</packaging>
	
 <name>Test</name>
  <url>http://maven.apache.org</url>

	 <dependencies>
		<dependency>
			<groupId>com.fascore.qa.AutomationFramework</groupId>
			<artifactId>Framework</artifactId>
			<version>3.0.8-SNAPSHOT</version>
		</dependency>
	</dependencies> 

	<build>
		<pluginManagement>
			<!-- Source directory configuration -->

			<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
              <artifactId>exec-maven-plugin</artifactId>
                <version>1.5.0</version>
                    <executions>
   					 <execution>
     				 <goals>
       				 <goal>java</goal>
     				 </goals>
    					</execution>
  					</executions>
					<configuration>
					  
						<mainClass>test.framework.ThreadInvocation</mainClass>
						 <classpathScope>test</classpathScope>
                    <arguments>
                    <argument>foo</argument>
                    <argument>bar</argument>
                     </arguments>
                     </configuration>
                     </plugin>
				<!-- Following plugin executes the testng tests -->
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-surefire-plugin</artifactId>
					<version>2.18.1</version>
					
					<configuration>
						<threadCount>1</threadCount>
						<reuseForks>false</reuseForks>
						<argLine>-Xmx1024m -XX:MaxPermSize=256m</argLine>
						<suiteXmlFiles>
						<suiteXmlFile>RunXML/${suiteFile}.xml</suiteXmlFile> 
						</suiteXmlFiles> 
						<systemProperties>
							<property>
								<name>env</name>
								<value>${env}</value>
							</property>
						</systemProperties>
						<systemProperties>
							<property>
								<name>BROWSER</name>
								<value>${BROWSER}</value>
							</property>
						</systemProperties>
						<systemProperties>
							<property>
								<name>testDataPath</name>
								<value>${testDataPath}</value>
							</property>
							</systemProperties>
						<systemProperties>
							<property>
								<name>CAPTURESCREENSHOT</name>
								<value>${CAPTURESCREENSHOT}</value>
							</property>
						</systemProperties>	
						<systemProperties>
							<property>
								<name>USERID</name>
								<value>${USERID}</value>
							</property>
						</systemProperties>
							<systemProperties>
							<property>
								<name>SYS_PASSWORD</name>
								<value>${SYS_PASSWORD}</value>
							</property>
						</systemProperties>
							<systemProperties>
							<property>
