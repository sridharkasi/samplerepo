								<value>${DB_PASSWORD}</value>
							</property>
						</systemProperties>	
						<systemProperties>
							<property>
								<name>executionMode</name>
								<value>${executionMode}</value>
							</property>
						</systemProperties>	
						
						<systemProperties>
							<property>
								<name>PlanCode</name>
								<value>${PlanCode}</value>
							</property>
						</systemProperties>	
						
						<systemProperties>
